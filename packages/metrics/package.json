{
    "name": "@proton/metrics",
    "description": "Metrics library",
    "scripts": {
        "check-types": "tsc",
        "generate-schema-types": "rimraf json-schema-registry types && git clone $SCHEMA_REPOSITORY && json2ts -i 'json-schema-registry/**/*.schema.json' -o types/ --no-additionalProperties",
        "generate-schema-types:mock": "rimraf types && json2ts -i 'mockSchemas/**/*.schema.json' -o types/ --no-additionalProperties",
        "lint": "eslint . --ext ts --quiet --cache"
    },
    "dependencies": {
        "json-schema-to-typescript": "^12.0.0"
    },
    "devDependencies": {
        "@proton/eslint-config-proton": "workspace:^",
        "eslint": "^8.36.0",
        "rimraf": "^4.4.0",
        "typescript": "^5.0.2"
    }
}
