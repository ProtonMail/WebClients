    "sideEffects": false,
    "scripts": {
        "test": "NODE_ENV=test karma start test/karma.conf.js",
        "testwatch": "npm test -- --auto-watch --no-single-run",
        "i18n:validate": "proton-i18n validate lint-functions lib",
