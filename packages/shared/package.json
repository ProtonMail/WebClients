        "@types/node": "^14.11.1",
        "@types/papaparse": "^5.2.5",
        "@types/ua-parser-js": "^0.7.33",
        "@types/webpack-env": "^1.16.2",
        "bip39": "github:ProtonMail/bip39#semver:^1.0.2",
