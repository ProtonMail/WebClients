{
    "name": "@proton/icons",
    "version": "0.0.2",
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": "./index.ts",
        "./types": "./types.ts",
        "./Icons": "./Icons.tsx",
        "./InlineIcons": "./InlineIcons.tsx",
        "./icons/*": "./icons/*.tsx"
    },
    "files": [
        "lib",
        "assets",
        "icons",
        "index.ts",
        "types.ts"
    ],
    "scripts": {
        "build": "yarn generate && tsc -P build.tsconfig.json",
        "generate": "tsx ./bin/generateReactIcons.ts",
        "lint": "eslint --quiet",
        "prepack": "yarn build"
    },
    "dependencies": {
        "@proton/utils": "workspace:^",
        "jsdom": "^27.0.0",
        "mustache": "^4.2.0",
        "prettier": "^3.6.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@types/mustache": "^4.2.6",
        "@types/react": "^18.3.26",
        "eslint": "^9.37.0",
        "tsx": "^4.20.6",
        "typescript": "^5.9.3"
    }
}
