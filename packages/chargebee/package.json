{
    "name": "@proton/chargebee",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "build": "tsc && NODE_ENV='production' vite build",
        "build:watch": "tsc && NODE_ENV='development' vite build --watch",
        "check-types": "tsc",
        "dev": "vite",
        "lint": "eslint --quiet",
        "preview": "vite preview",
        "serve": "serve dist -p 5173",
        "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
        "test": "jest",
        "test:dev": "jest --watch"
    },
    "dependencies": {
        "@proton/styles": "workspace:^"
    },
    "devDependencies": {
        "@fullhuman/postcss-purgecss": "^7.0.2",
        "@proton/jest-env": "workspace:^",
        "concurrently": "^8.2.2",
        "eslint": "^9.37.0",
        "jest": "^30.2.0",
        "postcss": "^8.5.6",
        "serve": "^14.2.5",
        "typescript": "^5.9.3",
        "vite": "^7.1.11",
        "vite-plugin-html": "^3.2.2",
        "vite-plugin-mkcert": "^1.17.9",
        "vite-plugin-singlefile": "^2.3.0"
    }
}
