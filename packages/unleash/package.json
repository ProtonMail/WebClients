{
    "name": "@proton/unleash",
    "description": "Unleash feature flags",
    "sideEffects": false,
    "scripts": {
        "check-types": "tsc",
        "lint": "eslint . --ext .js,.ts,.tsx --quiet --cache",
        "pretty": "prettier --write $(find . -path ./node_modules -prune -o -type f -name '*.js' -o -name '*.ts' -o -name '*.tsx')",
        "test": "jest",
        "test:ci": "jest --coverage --runInBand --ci",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "@unleash/proxy-client-react": "^5.0.0",
        "unleash-proxy-client": "^3.7.5"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.27.2",
        "@babel/preset-typescript": "^7.27.1",
        "@proton/eslint-config-proton": "workspace:^",
        "@types/react": "^18.3.23",
        "@types/react-dom": "^18.3.7",
        "eslint": "^8.57.1",
        "jest": "^29.7.0",
        "prettier": "^3.6.2",
        "typescript": "^5.8.3"
    },
    "peerDependencies": {
        "date-fns": "^3.6.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    }
}
