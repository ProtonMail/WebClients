{
    "name": "@proton/unleash",
    "description": "Unleash feature flags",
    "scripts": {
        "check-types": "tsc",
        "lint": "eslint . --ext .js,.ts,.tsx --quiet --cache",
        "pretty": "prettier --write $(find . -path ./node_modules -prune -o -type f -name '*.js' -o -name '*.ts' -o -name '*.tsx')",
        "test": "jest",
        "test:ci": "jest --coverage --runInBand --ci",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "@unleash/proxy-client-react": "^4.5.1",
        "unleash-proxy-client": "^3.7.2"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.26.8",
        "@babel/preset-typescript": "^7.26.0",
        "@proton/eslint-config-proton": "workspace:^",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "eslint": "^8.57.1",
        "jest": "^29.7.0",
        "prettier": "^3.5.0",
        "typescript": "^5.7.3"
    },
    "peerDependencies": {
        "date-fns": "^3.6.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    }
}
