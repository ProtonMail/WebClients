{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "sourceMap": true,
        "outDir": "dist",
        "lib": ["WebWorker", "DOM", "DOM.Iterable", "ESNext"],
        "types": ["global-types-monorepo", "webpack-env", "jest"]
    },
    "ts-node": {
        "compilerOptions": {
            "module": "commonjs",
            "moduleResolution": "node"
        }
    },
    "include": ["src/**/*"],
    "exclude": ["src/**/*.test.ts", "src/**/*.spec.ts"]
}
