{
    "name": "proton-verify",
    "description": "Human Verification",
    "license": "GPL-3.0",
    "scripts": {
        "build:web": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"../../tsconfig.webpack.json\" proton-pack build --appMode=standalone",
        "check-types": "tsc",
        "i18n:extract:web": "proton-i18n extract",
        "i18n:validate": "proton-i18n validate lint-functions",
        "i18n:validate:context:web": "proton-i18n validate",
        "postinstall": "proton-pack config",
        "lint": "eslint src --ext .js,.ts,.tsx --quiet --cache",
        "start": "cross-env TS_NODE_PROJECT=\"../../tsconfig.webpack.json\" proton-pack dev-server --appMode=standalone",
        "test": "jest",
        "test:ci": "jest --coverage --runInBand --ci",
        "test:dev": "jest --watch"
    },
    "dependencies": {
        "@proton/components": "workspace:^",
        "@proton/hooks": "workspace:^",
        "@proton/icons": "workspace:^",
        "@proton/polyfill": "workspace:^",
        "@proton/shared": "workspace:^",
        "@proton/styles": "workspace:^",
        "@proton/testing": "workspace:^",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/react-router-dom": "^5.3.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^5.3.4",
        "resize-observer": "^1.0.4"
    },
    "devDependencies": {
        "@proton/eslint-config-proton": "workspace:^",
        "@proton/i18n": "workspace:^",
        "@proton/pack": "workspace:^",
        "babel-jest": "^29.7.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-junit": "^16.0.0",
        "typescript": "^5.5.4"
    }
}
