{
    "name": "proton-lumo",
    "version": "0.1.0",
    "description": "Proton Lumo web application",
    "license": "GPL-3.0",
    "main": "index.html",
    "scripts": {
        "build:web": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"../../tsconfig.webpack.json\" proton-pack build --appMode=sso",
        "check-types": "tsc",
        "i18n:extract:web": "proton-i18n extract",
        "i18n:validate": "proton-i18n validate lint-functions",
        "i18n:validate:context:web": "proton-i18n validate",
        "lint": "eslint --quiet",
        "pretty": "prettier --write  $(find src/app -type f -name '*.js' -o -name '*.ts' -o -name '*.tsx')",
        "start": "cross-env TS_NODE_PROJECT=\"../../tsconfig.webpack.json\" proton-pack dev-server --appMode=standalone --no-error-logs",
        "test": "jest",
        "test:ci": "jest --coverage --runInBand --ci",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "@bjorn3/browser_wasi_shim": "^0.4.2",
        "@msgpack/msgpack": "^3.1.2",
        "@proton/atoms": "workspace:^",
        "@proton/crypto": "workspace:^",
        "@proton/drive": "workspace:^",
        "@proton/shared": "workspace:^",
        "@proton/styles": "workspace:^",
        "@reduxjs/toolkit": "^2.8.2",
        "@tiptap/extension-placeholder": "^2.26.1",
        "@tiptap/react": "^2.26.1",
        "@tiptap/starter-kit": "^2.26.1",
        "@types/jest": "^30.0.0",
        "@types/pdfjs-dist": "^2.10.378",
        "@types/tinycolor2": "^1.4.6",
        "@types/toposort": "^2.0.7",
        "clsx": "^2.1.1",
        "jest": "^30.1.2",
        "jest-environment-jsdom": "^30.1.2",
        "json-stable-stringify": "^1.3.0",
        "lodash": "^4.17.21",
        "lottie-react": "^2.4.1",
        "p-limit": "^6.2.0",
        "pdfjs-dist": "^4.10.38",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-markdown": "^9.1.0",
        "react-redux": "^9.2.0",
        "react-router": "^5.3.4",
        "react-router-dom": "^5.3.4",
        "react-syntax-highlighter": "^15.6.6",
        "react-textarea-autosize": "^8.5.9",
        "react-virtuoso": "^4.14.0",
        "redux-saga": "^1.3.0",
        "remark-gfm": "^4.0.1",
        "tinycolor2": "^1.6.0",
        "tiptap-markdown": "^0.8.10",
        "toposort": "^2.0.2",
        "ts-jest": "^29.4.1",
        "ttag": "^1.8.12",
        "turndown": "^7.2.1",
        "uuidv4": "^6.2.13",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@peculiar/webcrypto": "^1.5.0",
        "@proton/i18n": "workspace:^",
        "@proton/pack": "workspace:^",
        "@proton/testing": "workspace:^",
        "@types/node": "^22.18.0",
        "@types/react": "^18.3.24",
        "@types/react-dom": "^18.3.7",
        "@types/react-syntax-highlighter": "^15.5.13",
        "@vitejs/plugin-react-swc": "^3.11.0",
        "autoprefixer": "^10.4.21",
        "babel-plugin-transform-import-meta": "^2.3.3",
        "cross-env": "^7.0.3",
        "eslint": "^9.34.0",
        "eslint-config-prettier": "^10.1.8",
        "fake-indexeddb": "^6.2.2",
        "msw": "^2.11.1",
        "postcss": "^8.5.6",
        "sass": "1.79.5",
        "typescript": "^5.9.2",
        "vite": "^5.4.19"
    },
    "msw": {
        "workerDirectory": [
            "public"
        ]
    }
}
