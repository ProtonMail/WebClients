{
    "name": "proton-storybook",
    "description": "",
    "keywords": [],
    "license": "GPL-3.0",
    "author": "",
    "main": "index.js",
    "scripts": {
        "check-types": "tsc",
        "deploy": "netlify deploy --auth $NETLIFY_AUTH_TOKEN --site $NETLIFY_SITE_ID --dir applications/storybook/storybook-static --alias $NETLIFY_ALIAS",
        "deploy:prod": "netlify deploy --auth $NETLIFY_AUTH_TOKEN --site $NETLIFY_SITE_ID --prod --dir applications/storybook/storybook-static",
        "lint": "eslint --quiet",
        "pretty": "prettier --write $(find . -path ./node_modules -prune -o -path ./src -type f -name '*.js' -o -name '*.ts' -o -name '*.tsx')",
        "start": "storybook dev -p 6006",
        "storybook:build": "storybook build --loglevel warn --quiet",
        "storybook:build:serve": "http-server ./storybook-static --cors --port 3000 --silent",
        "storybook:build:wait": "wait-on http://localhost:3000",
        "storybook:dev": "storybook dev -p 6006"
    },
    "dependencies": {
        "@proton/components": "workspace:^",
        "@proton/eslint-config-proton": "workspace:^",
        "@proton/icons": "workspace:^",
        "@proton/pack": "workspace:^",
        "@proton/shared": "workspace:^",
        "@proton/styles": "workspace:^",
        "@types/react": "^18.3.23",
        "@types/react-dom": "^18.3.7",
        "@types/react-router-dom": "^5.3.3",
        "cross-env": "^7.0.3",
        "eslint": "^9.33.0",
        "prettier": "^3.6.2",
        "prop-types": "15.8.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^5.3.4",
        "ts-loader": "^9.5.2",
        "typescript": "^5.9.2",
        "webpack": "^5.101.3"
    },
    "devDependencies": {
        "@babel/core": "^7.28.3",
        "@proton/icons": "workspace:^",
        "@storybook/addon-actions": "^8.6.14",
        "@storybook/addon-essentials": "^8.6.14",
        "@storybook/addon-links": "^8.6.14",
        "@storybook/addon-storysource": "^8.6.14",
        "@storybook/react": "^8.6.14",
        "@storybook/react-webpack5": "^8.6.14",
        "@storybook/source-loader": "^8.6.14",
        "babel-loader": "^10.0.0",
        "eslint-plugin-storybook": "^0.12.0",
        "netlify-cli": "17.38.1",
        "storybook": "^8.6.14"
    }
}
