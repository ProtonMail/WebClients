{
    "name": "proton-storybook",
    "description": "",
    "keywords": [],
    "license": "GPL-3.0",
    "author": "",
    "main": "index.js",
    "scripts": {
        "storybook": "start-storybook -p 6006 -s src/assets/favicons --docs --no-manager-cache",
        "storybook-raw": "start-storybook -p 6006 -s src/assets/favicons",
        "start": "proton-pack config && start-storybook -p 6006 -s src/assets/favicons",
        "lint": "eslint src --ext .js,.ts,.tsx --quiet",
        "pretty": "prettier --write $(find . -path ./node_modules -prune -o -path ./src -type f -name '*.js' -o -name '*.ts' -o -name '*.tsx')",
        "deploy": "netlify deploy --auth $NETLIFY_AUTH_TOKEN --site $NETLIFY_SITE_ID --dir storybook-static --alias $NETLIFY_ALIAS",
        "deploy:prod": "netlify deploy --auth $NETLIFY_AUTH_TOKEN --site $NETLIFY_SITE_ID --prod --dir storybook-static",
        "check-types": "tsc",
        "build": "build-storybook -s src/assets/favicons --docs",
        "postinstall": "proton-pack config"
    },
    "dependencies": {
        "@proton/components": "workspace:packages/components",
        "@proton/eslint-config-proton": "workspace:packages/eslint-config-proton",
        "@proton/pack": "workspace:packages/pack",
        "@proton/shared": "workspace:packages/shared",
        "@proton/styles": "workspace:packages/styles",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^7.9.0",
        "prettier": "^2.1.2",
        "prop-types": "15.7.2",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "ts-loader": "^9.2.5",
        "typescript": "^4.0.5",
        "webpack": "^5.50.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@storybook/addon-actions": "^6.3.7",
        "@storybook/addon-essentials": "^6.3.7",
        "@storybook/addon-links": "^6.3.7",
        "@storybook/addon-storysource": "^6.3.7",
        "@storybook/builder-webpack5": "^6.3.7",
        "@storybook/manager-webpack5": "^6.3.7",
        "@storybook/react": "^6.3.7",
        "@storybook/source-loader": "^6.3.7",
        "babel-loader": "^8.2.2",
        "netlify-cli": "^2.67.4"
    }
}
